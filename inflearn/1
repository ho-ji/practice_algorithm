#include <iostream>
#include <vector>

using namespace std;

int main(){
	int N;
	
	vector<int> list;

	cin >> N;

	for(int i = 0; i < N; i++){
	 	int a;
		cin >> a;
		list.push_back(a);
		
	}
	
	cout << 0 << " ";

	for(int i = 1; i < N; i++){
		for(int j = i-1; j >= 0; j--){
			if(list[i] < list[j]){
				cout << j+1 << " ";
				break;
			}
		}
		cout << 0 << " ";
	}	
	



	return 0;
}
